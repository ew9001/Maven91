<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


 
<suite name="Firefox Contact Form" verbose="1">
    <parameter name="browser" value="firefox"/>
    <test name="Contact Form">
    
        <classes>
            <class name="test.nestle.ContactUS"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    <test name="Create Account Form">
    
        <classes>
            <class name="test.nestle.createaccount.CreateAccount"/>
        </classes>
 
        <groups>
            <run>
                <include name="newaccount"/>
            </run>
        </groups>
    </test>
</suite>