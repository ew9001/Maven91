<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
 
<suite name="Android Contact Form"  verbose="1">
    <parameter name="browser" value="chrome"/>
 
  
    <test name="Contact Form" >
         
  <groups> 
    <run> 
      <include name="create"/> 
    </run> 
  </groups> 
  <classes> 
    <class name="test.nestle.ContactUS"/> 
  </classes> 
</test>
    
     
     <test name="Forgot Password"> 
  <groups> 
    <run> 
      <include name="user"/> 
    </run> 
  </groups> 
  <classes> 
    <class name="password.ForgotPassword"/> 
  </classes> 
</test>
     
     
     
     
    <test name="Existing User"> 
  <groups> 
    <run> 
      <include name="user"/> 
    </run> 
  </groups> 
  <classes> 
    <class name="existinguser.User"/> 
  </classes> 
</test>
</suite>