<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
 
<suite name="Android Contact Form" verbose="1">
    <parameter name="param"  preserve-order="false" value="chrome"/>
    <test name="Contact Form">
    
        <classes>
            <class name="test.nestle.ContactUS"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
     <test name="Forgot Password">
    	<parameter name = "browser" value = "chrome"/>
        <classes>
            <class name="password.ForgotPassword"/>
        </classes>
 
        <groups>
            <run>
                <include name="forgotpassword"/>
            </run>
        </groups>
    </test>
</suite>